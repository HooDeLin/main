<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<HBox xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Region minWidth="20.0" prefWidth="20.0" HBox.hgrow="NEVER" />
      <Label fx:id="lineNum" text="Line Number" />
      <Label fx:id="description" text="Description" wrapText="true">
         <padding>
            <Insets left="5.0" />
         </padding>
      </Label>
      <Region minWidth="10.0" HBox.hgrow="ALWAYS" />
      <Label fx:id="time" alignment="CENTER_RIGHT" contentDisplay="TEXT_ONLY" minWidth="150.0" text="Time" textAlignment="RIGHT" HBox.hgrow="ALWAYS" />
   </children>
</HBox>
